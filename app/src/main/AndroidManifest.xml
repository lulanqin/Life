<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cruse.life">
    
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.NewMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.FoodListActivity"/>
        <activity android:name=".activity.FoodDetailAtivity"/>
        <activity android:name=".activity.BookListActivity"/>
        <activity android:name=".activity.BookDetailAtivity"/>
        <activity android:name=".activity.ChapterActivity"/>
        <activity android:name=".activity.FocusListActivity"/>
        <activity android:name=".activity.FocusDetailAtivity"/>
        <activity android:name=".activity.NewsListActivity"/>
        <activity android:name=".activity.NewsDetailAtivity"/>
        <activity android:name=".activity.FocusUpdateDetailActivity"/>


    </application>

</manifest>
